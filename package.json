{
  "name": "@sidislive/genesis-world-gen",
  "version": "1.1.2",
  "description": "World Generator 2D for Jericho Discord Bot - Private Sub-Project",
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "types": "./typings/index.d.ts",
  "files": [
    "src",
    "typings"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs"
    }
  },
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "commit": "git-cz",
    "semantic-release": "semantic-release --branches main",
    "push": "git add . && npm run commit && git push",
    "docs": "jsdoc node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.16.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "jsdoc": "^3.6.7",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-skyceil": "^2.0.0",
    "nyc": "^15.1.0",
    "semantic-release": "^19.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "youtube-scrapper",
    "video-extractor",
    "music-downloader",
    "youtube",
    "spotify",
    "soundcloud",
    "facebook",
    "music-extractor",
    "soundcloud-scrapper",
    "spotify-scrapper",
    "youtube-extractor",
    "youtube-downloader",
    "play-dl",
    "jericho-player"
  ],
  "engines": {
    "node": ">= 16.6.0",
    "npm": ">=7.0.0"
  },
  "directories": {
    "example": "example"
  },
  "author": "Sid is Live YT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jericho-Community/genesis-world-gen.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SJericho-Community/genesis-world-gen/issues"
  },
  "homepage": "https://github.com/Jericho-Community/genesis-world-gen#readme"
}
